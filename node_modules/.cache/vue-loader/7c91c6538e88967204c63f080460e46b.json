ion>\n    </select>月\n    <select v-model=\"day\">\n    <option v-for=\"n in 31\" :value=\"n\" :key=\"n\">{{ n }}</option>\n    </select>日\n    </div>\n  </div>             \n</div>\n<router-link to=\"/about\"><button class=\"button is-primary\" id=\"next-btn\">次へ進む</button></router-link>\n</div>\n\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      year: 2000,\n      month: 1,\n      date: 1,\n      day: 1,\n      radio:'',\n      radio2:'',\n      radio3:'',\n      nengoes: []\n    };\n  },\n  mounted() {\n    this.nengoes = this.genereate();\n  },\n  methods: {\n    genereate() {\n      const nengoes = [];\n      for (let y=2020; y>1920; y--) {\n        if (y > 2018) {\n          nengoes.push( {\"year\": y, \"label\": `${y} (令和${y-2018}年)`} );\n        } else if (y > 1988) {\n          nengoes.push( {\"year\": y, \"label\": `${y} (平成${y-1988}年)`} );\n        } else if (y > 1925) {\n          nengoes.push( {\"year\": y, \"label\": `${y} (昭和${y-1925}年)`} );\n        } else if (y > 1911) {\n          nengoes.push( {\"year\": y, \"label\": `${y} (大正${y-1911}年)`} );\n        }\n      }\n      return nengoes;\n    }\n  }\n};\n</script>\n"]}]}